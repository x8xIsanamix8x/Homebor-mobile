{
  "expo": {
    "name": "Homebor",
    "description": "Homebor has provided its homestay users with an application in which they can organize, promote and manage their availability by rooms autonomously. At the same time, to propose an estimated cost of your rooms per week, to have a reporting system for your students and to have real-time payment control",
    "slug": "Homebor",
    "version": "1.0.9",
    "orientation": "portrait",
    "icon": "./assets/img/logos/iphone.png",
    "splash": {
      "image": "./assets/img/logos/13.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.xeondevelopersvzla.homebor",
      "googleServicesFile": "./GoogleService-Info.plist",
      "buildNumber": "35.0.0",
      "supportsTablet": true,
      "usesIcloudStorage" : true,
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses the camera to capture and upload user profile image and listing image.",
        "NSPhotoLibraryUsageDescription": "This app needs access to the photo library to select and upload user profile image and listing image."
      }
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/img/logos/8.png",
          "color": "#ffffff"
        }
      ]
    ],
    "android": {
      "package": "com.xeondevelopersvzla.homebor",
      "googleServicesFile": "./google-services.json",
      "versionCode": 35,
      "adaptiveIcon": {
        "foregroundImage": "./assets/img/logos/14.png",
        "backgroundColor": "#FFFFFF"
      },
      "useNextNotificationsApi": true,
      "permissions": ["RECEIVE_BOOT_COMPLETED", 
        "CAMERA",
        "NOTIFICATIONS",
        "READ_INTERNAL_STORAGE",
        "WRITE_INTERNAL_STORAGE",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ]
    },
    "web": {
      "favicon": "./assets/img/logos/8.png"
    },
    "notification": {
      "icon": "./assets/img/logos/8.png",
      "iosDisplayInForeground": true
    }
  }
}
