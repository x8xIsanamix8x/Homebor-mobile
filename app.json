{
  "expo": {
    "name": "Homebor",
    "slug": "Homebor",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/1.png",
    "splash": {
      "image": "./assets/13.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.xeon.homebor",
      "buildNumber": "1.0.0",
      "supportsTablet": true,
      "usesIcloudStorage" : true
    },
    "android": {
      "package": "com.xeon.homebor",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/1.png",
        "backgroundColor": "#FFFFFF"
      },
      "useNextNotificationsApi": true,
      "permissions": ["RECEIVE_BOOT_COMPLETED", 
        "CAMERA",
        "READ_INTERNAL_STORAGE",
        "WRITE_INTERNAL_STORAGE",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ]
    },
    "web": {
      "favicon": "./assets/1.png"
    },
    "notification": {
      "icon": "./assets/1.png",
      "iosDisplayInForeground": true
    }
  }
}
